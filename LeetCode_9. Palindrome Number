import java.util.*;
class Solution {
    public boolean isPalindrome(int x) {
        int rev=0,num=x;
        while(x!=0 && x>0){
            int last_digit=x%10;
            x=x/10;
            rev=rev*10+last_digit;           
        }
        if(num==rev){
            return true;
        }
        else{
            return false;
        }
    }
}
