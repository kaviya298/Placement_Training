import java.util.*;
class Solution {
    public int fib(int n) {
        int sum=0;
        int []b=new int[n+1];
        
        if(n==1)
        return 1;
        if(n==0)
        return 0;
        if(n>1){
        b[0]=0;
        b[1]=1;
        for(int i=2;i<=n;i++){
            b[i]=b[i-1]+b[i-2];
        }
    }
    return b[n];
    }
}
